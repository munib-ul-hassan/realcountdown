<div class="container-fluid" id="myDIV">
   <% properties.forEach(property=> { %>
     

      <%
      day = 0
      hour = 0
      minute = 0
      second = 0      
                  const date = new Date();
                 
                  let ct = new Date(property.countdownOverAt);
             
                  const dateTime = ct.getTime() - date.getTime()
                  if(dateTime > 0){
                  const diff = dateTime / (1000 * 3600 * 24)
                  
                
                  let c = diff * 24

                  seconds = Number(c) * 3600;
                   day = Math.floor(seconds / (3600*24));
                   hour = Math.floor(seconds % (3600*24) / 3600);
                   minute = Math.floor(seconds % 3600 / 60);
                   second = Math.floor(seconds % 60);
              }   
              
                  %> 

        
     
   <div class="row">
      <div class="col-md-12 ">
         <div class="text-center">
            <img style="width: 150px;" src="/images/BIDDER.png" alt="" />
         </div>
         <div class="text-center pt-3">
            <img style="width: 150px" src="/images/Count_BatVersion.png" alt="" />
         </div>
         <div class=" d-flex justify-content-center pt-2">
            <div class="m-2">
               <span id="day" style="background-color: black;padding: 8px; font-size: 20px;color: white; border-radius: 10px; "><%= day %></span>
               <p class="pt-2 text-center">days</p>
            </div>
            <div class="m-2">
               <span id="hour" style="background-color: black;padding: 8px; font-size: 20px;color: white; border-radius: 10px;"><%= hour %></span>
               <p class="pt-2 text-center">hrs</p>
            </div>
            <div class="m-2">
               <span id="minute" style="background-color: black;padding: 8px; font-size: 20px;color: white; border-radius: 10px;"><%= minute %></span>
               <p class="pt-2 text-center">min</p>
            </div>
            <div class="m-2">
               <span id="second" style="background-color: black;padding: 8px; font-size: 20px;color: white; border-radius: 10px;"><%= second %></span>
               <p class="pt-2 text-center">sec</p>
            </div>
         </div>
      </div>
   </div>
   <div class="row">
      <div class="col-md-1"></div>
      <div class="col-md-4">
         <div class="text-center">
            <h2>Seller</h2>
         </div>
         <div class="text-center">
            <img style="width: 250px; height: 150px;" src="/images/buyer-countdown.png" alt="">
         </div>
      </div>
      <div class="col-md-4 offset-md-2 pt-5">
         <div class="card ">
            <div class="card-body">
               <div class="row no-gutters align-items-center">
                  <div>
                     <h2> <%= property.agentBids[0].agentBid.commision %>%</h2>
                  </div>
                  <div class="col-auto ml-3">
                     <img style="
                        width: 70px;
                        border-radius: 50%;
                        border: 1px solid lightgray;
                        " src="/images/StandingCount.png" alt="" />
                  </div>
                  <div class="col mr-2 pl-4">
                     <div class="
                        text-xs
                        font-weight-bold
                        text-success text-uppercase
                        ">
                       <h6>  <%= property.agentBids[0].agentBid.name %></h6> 
                     </div>
                     <div class="
                        text-xs
                        pt-2
                        font-weight-bold
                        text-success text-uppercase
                        ">
                        <%= property.agentBids[0].agentBid.brokeragePhone %>
                     </div>
                     <div class="
                        text-xl
                        pt-2
                        font-weight-bold
                        text-success text-uppercase
                        ">
                        *****
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <div class="col-md-1"></div>
   </div>
